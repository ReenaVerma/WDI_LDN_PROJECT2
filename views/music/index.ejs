<main class="container">
  <h1 class="title is-black">Latest Popups</h1>
  <div class="columns is-multiline">

    <% music.forEach(popup => { %>
    <div class="column is-one-quarter-desktop is-half-tablet animated fadeInUp">
    <a href="/music/<%= popup._id %>">
    <div class="card">
      <div class="card-image">
        <figure class="image">
          <img src="<%= popup.image %>" alt="<%= popup.image %>">
        </figure>
      </div>

      <div class="card-content">
            <p class="title is-4 is-black"><%= popup.title %></p>
            <p class="subtitle is-6 is-black"><%= popup.where %></p>

      </div>
    </div>
    </a>
    </div>

    <% }) %>
  </div>
</main>
