<section class="hero is-small homepage hero-head animated fadeIn is-bold">
  <div class="hero-body">
    <div class="container has-text-centered">
      <h1 class="title has-text-white">latest listings in London</h1>
    </div>
  </div>
</section>

<main class="container">
  <h2 class="is-size-2 is-black">Latest Popups</h2>
  <div class="columns is-multiline">

    <% popup.forEach(popup => { %>
    <div class="column is-one-quarter-desktop is-half-tablet animated fadeInUp">
    <a href="/popuplisting/<%= popup._id %>">
    <div class="card">
      <div class="card-image">
        <figure class="image">
          <img src="<%= popup.image %>" alt="<%= popup.image %>">
        </figure>
      </div>

      <div class="card-content">
            <p class="title is-4 is-black"><%= popup.title %></p>
            <p class="subtitle is-6 is-black"><%= popup.where %></p>

      </div>
    </div>
    </a>
    </div>

    <% }) %>
  </div>
</main>
